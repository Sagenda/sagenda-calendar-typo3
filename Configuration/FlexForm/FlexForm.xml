<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure>
    <sheets>
        <general>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Select Option</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.token>
                        <TCEforms>
                            <label>Authentication code (Token)</label>
                            <config>
                                <checkbox>1</checkbox>
                                <default></default>
                                <type>input</type>
                                <size>40</size>
                            </config>
                        </TCEforms>
                    </settings.token>
                    <settings.date>
                        <TCEforms>
                            <label>Date</label>
                            <config>
                                <checkbox>1</checkbox>
                                <default>DD-MMMM-YYYY</default>
                                <type>input</type>
                                <size>15</size>
                            </config>
                        </TCEforms>
                    </settings.date>
                    <settings.time>
                        <TCEforms>
                            <label>Time</label>
                            <config>
                                <checkbox>1</checkbox>
                                <default>HH:mm</default>
                                <type>input</type>
                                <size>15</size>
                            </config>
                        </TCEforms>
                    </settings.time>

	                <settings.weekstarton>
                        <TCEforms>
                            <label>Week start on</label>
                            <onChange>reload</onChange>
                            <config>
                                <type>select</type>
                                <renderType>selectSingle</renderType>
                                <items type="array">
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">Sunday</numIndex>
                                        <numIndex index="1">0</numIndex>
                                    </numIndex>
                                    <numIndex index="2" type="array">
                                        <numIndex index="0">Monday</numIndex>
                                        <numIndex index="1">1</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.weekstarton>

                    <settings.defaultview>
                        <TCEforms>
                            <label>Default view</label>
                            <onChange>reload</onChange>
                            <config>
                                <type>select</type>
                                <renderType>selectSingle</renderType>
                                <default>month</default>
                                <items type="array">
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">Day</numIndex>
                                        <numIndex index="1">day</numIndex>
                                    </numIndex>
                                    <numIndex index="2" type="array">
                                        <numIndex index="0">Month</numIndex>
                                        <numIndex index="1">month</numIndex>
                                    </numIndex>
                                    <numIndex index="3" type="array">
                                        <numIndex index="0">Year</numIndex>
                                        <numIndex index="1">year</numIndex>
                                    </numIndex>
                                    <numIndex index="4" type="array">
                                        <numIndex index="0">Agenda</numIndex>
                                        <numIndex index="1">agenda</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.defaultview>
                    <settings.removeagendaviewbutton>
                        <TCEforms>
                            <label>Remove Agenda View Button</label>
                            <config>
                                <checkbox>1</checkbox>
                                <type>check</type>
                                <items type="array">
                                   <numIndex index="1" type="array">
                                      <numIndex index="0">Remove Agenda view button</numIndex>
                                      <numIndex index="1">1</numIndex>
                                   </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.removeagendaviewbutton>
                    <settings.removemonthviewbutton>
                        <TCEforms>
                            <label>Remove Month View Button</label>
                            <config>
                                <checkbox>1</checkbox>
                                <type>check</type>
                                <items type="array">
                                   <numIndex index="1" type="array">
                                      <numIndex index="0">Remove Month view button</numIndex>
                                      <numIndex index="1">1</numIndex>
                                   </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.removemonthviewbutton>
                    <settings.removeweekviewbutton>
                        <TCEforms>
                            <label>Remove Week View Button</label>
                            <config>
                                <checkbox>1</checkbox>
                                <type>check</type>
                                <items type="array">
                                   <numIndex index="1" type="array">
                                      <numIndex index="0">Remove Week view button</numIndex>
                                      <numIndex index="1">1</numIndex>
                                   </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.removeweekviewbutton>
                    <settings.removedayviewbutton>
                        <TCEforms>
                            <label>Remove Day View Button</label>
                            <config>
                                <checkbox>1</checkbox>
                                <type>check</type>
                                <items type="array">
                                   <numIndex index="1" type="array">
                                      <numIndex index="0">Remove Day view button</numIndex>
                                      <numIndex index="1">1</numIndex>
                                   </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.removedayviewbutton>

				</el>
            </ROOT>
        </general>
    </sheets>
</T3DataStructure>

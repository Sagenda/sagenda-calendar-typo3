
plugin.tx_sagendacalendar_sagendacalendar {
    view {
        templateRootPaths.0 = EXT:sagenda_calendar/Resources/Private/Templates/
        templateRootPaths.1 = {$plugin.tx_sagendacalendar_sagendacalendar.view.templateRootPath}
        partialRootPaths.0 = EXT:sagenda_calendar/Resources/Private/Partials/
        partialRootPaths.1 = {$plugin.tx_sagendacalendar_sagendacalendar.view.partialRootPath}
        layoutRootPaths.0 = EXT:sagenda_calendar/Resources/Private/Layouts/
        layoutRootPaths.1 = {$plugin.tx_sagendacalendar_sagendacalendar.view.layoutRootPath}
    }
}

page {
    includeJSLibs {
        runtime = EXT:sagenda_calendar/Resources/Public/JavaScript/runtime.js
        polyfills = EXT:sagenda_calendar/Resources/Public/JavaScript/polyfills.js
        main = EXT:sagenda_calendar/Resources/Public/JavaScript/main.js
    }
}